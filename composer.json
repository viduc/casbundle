{
    "name": "viduc/casbundle",
    "type": "symfony-bundle",
    "description": "bundle d'authentification cas pour les applications symfony",
    "keywords": ["cas"],
    "homepage": "https://github.com/viduc",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tristan Fleury",
            "email": "viduc@mail.fr"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "jasig/phpcas": "^1.3||^1.4",
        "symfony/framework-bundle": "^5.1||^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.5|^9.0",
        "symfony/browser-kit": "3.4.42"
    },
    "autoload": {
        "psr-4": { "Viduc\\CasBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
