{
    "name": "viduc/casbundle",
    "type": "symfony-bundle",
    "description": "bundle d'authentification cas pour les applications symfony",
    "keywords": ["cas"],
    "homepage": "https://github.com/viduc",
    "license": "Apache-2.0",
    "version": "1.0.4",
    "authors": [
        {
            "name": "Tristan Fleury",
            "email": "tristan.fleury@univ-grenoble-alpes.fr"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "jasig/phpcas": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "8.5.8",
        "symfony/browser-kit": "3.4.42"
    },
    "autoload": {
        "psr-4": { "Viduc\\CasBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
